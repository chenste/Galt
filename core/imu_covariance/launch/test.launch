<launch>
  <!-- launch imu to test -->
  <include file="$(find imu_3dm_gx4)/launch/imu.launch"/>

  <node pkg="imu_covariance" name="imu_covariance" type="imu_covariance"
    output="screen">
    <param name="config_path" type="string" value="$(find imu_covariance)/config/sample.yml"/>
    <remap from="~imu_in" to="/imu_3dm_gx4/imu"/>
    <remap from="~magnetic_field_in" to="/imu_3dm_gx4/magnetic_field"/>
    <remap from="~pressure_in" to="/imu_3dm_gx4/pressure"/>
  </node>

</launch>
