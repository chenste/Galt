<!-- -*- mode: XML -*- -->
<!-- rostest launch file for bluefox2
     This unit test uses rostest, because the bluefox2
     constructor requires a node handle. -->

<launch>

    <arg name="serial" type="string" value="25000495" />
    <arg name="calibration_file" default="calib_$(arg serial).yml" />
    <param name="serial"
    <param name="calibration_url" type="string" value="package://bluefox2/config/$(arg calibration_file)" />

    <!-- start unit-test node -->
    <test pkg="bluefox2" type="rtest_mono" name="rtest_mono"
        test-name="rtest_mono" time-limit="10.0"/>

</launch>
